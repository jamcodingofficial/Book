#include <iostream>
#include <stack>

using namespace std;

int main(){
    stack<int> s;
    
    s.push(30);
    s.push(20);
    s.push(10);
    
    cout << "size : " << s.size() << "\n";
    
    while(s.empty() != 1){ // !s.empty()
        cout << s.top() << " ";
        s.pop();
    }
    
    return 0;
}
