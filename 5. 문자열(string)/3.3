#include <iostream>
#include <string>

using namespace std;

int main(){
    string color = "skyblue";
    
    cout << "color : " << color << "\n";
    cout << "color.length() : " << color.length() << "\n\n";
    
    color += " green";
    
    cout << "color : " << color << "\n";
    cout << "color.length() : " << color.length() << "\n";
    
    return 0;
}
/*
color : skyblue
color.length() : 7

color : skyblue green
color.length() : 13
*/