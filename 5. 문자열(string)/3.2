#include <iostream>
#include <string>

using namespace std;

int main(){
    string color = "skyblue";
    
    cout << "color.front() : " << color.front() << "\n";
    cout << "color[0] : " << color[0] << "\n";
    
    cout << "color.back() : " << color.back() << "\n";
    cout << "color[color.length()-1] : " << color[color.length()-1] << "\n";
    
    return 0;
}
/*
color.front() : s
color[0] : s
color.back() : e
color[color.length()-1] : e
*/