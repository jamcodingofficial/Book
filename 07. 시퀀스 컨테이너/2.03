#include <iostream>
#include <vector>

using namespace std;

int main(){
    
    // 벡터 생성 첫 번째 방법
    vector<int> v1 = {10, 30, 20, 50, 40};
    cout << "v1 결과 : ";
    for(int i=0; i<v1.size(); i++){
        cout << v1[i] << " ";
    }
    cout << "\n";
    
    // 벡터 생성 두 번째 방법
    vector<int> v2(8);
    cout << "v2 결과 : ";
    for(int i=0; i<v2.size(); i++){
        cout << v2[i] << " ";
    }
    cout << "\n";
    
    // 벡터 생성 세 번째 방법
    vector<int> v3(8,2);
    cout << "v3 결과 : ";
    for(int i=0; i<v3.size(); i++){
        cout << v3[i] << " ";
    }
    
    return 0;
}
/*
v1 결과 : 10 30 20 50 40 
v2 결과 : 0 0 0 0 0 0 0 0 
v3 결과 : 2 2 2 2 2 2 2 2
*/