#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> v;
    
    for(int i=0; i<20; i++){
        v.push_back(0);
        cout << "size : " << v.size() << ", capacity : " << v.capacity() << "\n";
    }
    
    return 0;
}
