#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> v;
    
    v.push_back(10);
    v.push_back(30);
    v.push_back(20);
    v.push_back(50);
    v.push_back(40);
    
    for(int i=0; i<v.size(); i++){
        cout << i << "번째 원소 : " << v[i] << "\n";
    }
    
    return 0;
}
/*
0번째 원소 : 10
1번째 원소 : 30
2번째 원소 : 20
3번째 원소 : 50
4번째 원소 : 40
*/