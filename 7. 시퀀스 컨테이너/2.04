#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> v = {20, 40, 30, 50, 10};
    
    cout << "실행 전 : ";
    for(int i=0; i<v.size(); i++){
        cout << v[i] << " ";
    }
    cout << "\n";
    
    v.pop_back();
    
    cout << "실행 후 : ";
    for(int i=0; i<v.size(); i++){
        cout << v[i] << " ";
    }
    cout << "\n";
    
    v.pop_back();
    v.pop_back();
    
    cout << "실행 후 : ";
    for(int i=0; i<v.size(); i++){
        cout << v[i] << " ";
    }
    
    return 0;
}
/*
실행 전 : 20 40 30 50 10 
실행 후 : 20 40 30 50 
실행 후 : 20 40
*/
