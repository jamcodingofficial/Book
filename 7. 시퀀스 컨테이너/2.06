#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> v;
    
    v.push_back(10);
    v.push_back(30);
    v.push_back(20);
    v.push_back(50);
    v.push_back(40);
    
    cout << "size : " << v.size() << "\n";
    cout << "capacity : " << v.capacity() << "\n";
    
    return 0;
}
/*
size : 5
capacity : 8
*/