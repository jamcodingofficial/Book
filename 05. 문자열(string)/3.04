#include <iostream>
#include <string>

using namespace std;

int main(){
    string color;
    cout << "color.empty() : " << color.empty() << ", color : " << color << "\n";
    
    color = "skyblue";
    cout << "color.empty() : " << color.empty() << ", color : " << color << "\n";
    
    color = "";
    cout << "color.empty() : " << color.empty() << ", color : " << color << "\n";
    
    return 0;
}
