#include <iostream>
#include <string>

using namespace std;

int main(){
    int n, cnt = 1;
    string str;
    
    cin >> n;
    
    for(int i=0; i<n; i++){
        getline(cin ,str);
        cout << cnt << "번째 결과 : " << str << "\n";
        cnt++;
    }
    
    return 0;
}
