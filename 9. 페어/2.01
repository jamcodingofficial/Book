#include <iostream>
#include <algorithm>

using namespace std;

int main(){
    pair<int, int> p1 = make_pair(10, 20); // pair<int, int> p1 = {10, 20};
    cout << p1.first << ", " << p1.second << "\n";
    
    pair<string, int> p2;
    p2 = make_pair("Dennis", 100); // p2 = {"Dennis", 100};
    cout << p2.first << ", " << p2.second << "\n";
    
    pair<double, int> p3;
    p3.first = 3.14;
    p3.second = 1592;
    cout << p3.first << ", " << p3.second << "\n";
    
    return 0;
}
/*
10, 20
Dennis, 100
3.14, 1592
*/