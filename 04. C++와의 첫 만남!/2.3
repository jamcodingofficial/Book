#include <iostream>

int main(){
    int age = 20;
    char name[20] = "Dennis";
    
    std::cout << "Hello, World!" << std::endl;
    std::cout << "저의 나이는 " << age << "살 입니다." << std::endl;
    std::cout << "저의 이름은 " << name << "이며, 나이는 " << age << "살 입니다." << std::endl;
    
    return 0;
}
/*
Hello, World!
저의 나이는 20살 입니다.
저의 이름은 Dennis이며, 나이는 20살 입니다.
*/